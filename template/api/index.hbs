import request from '@/utils/request'

export function getList(params) {
  return request({
    url: '/{{folderName}}/{{ filename }}/getList',
    method: 'get',
    params
  })
}

export function doEdit(data) {
  return request({
    url: '/{{folderName}}/{{ filename }}/doEdit',
    method: 'post',
    data
  })
}

export function doDelete(data) {
  return request({
  url: '/{{folderName}}/{{ filename }}/doDelete',
    method: 'post',
    data
  })
}
